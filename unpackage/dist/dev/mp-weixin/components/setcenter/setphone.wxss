.orders {
  background-color: #fff;
  margin-top: 10rpx;
}
.orders .orders-title-name {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  line-height: 80rpx;
  padding: 0 20rpx;
}
.orders .orders-title-name text {
  font-size: 30rpx;
  font-weight: bold;
}
.orders .orders-title-name .mintext {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #999;
}
.orders .orders-title-name .mintext text {
  font-size: 25rpx;
  font-weight: normal;
}

